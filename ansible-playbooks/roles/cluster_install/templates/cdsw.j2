{
    "refName": "CDSW-1",
    "serviceType": "CDSW",
    "serviceConfigs": [
        {
            "name": "yarn_service",
            "ref": "YARN-1"
        }, {
            "name": "hdfs_service",
            "ref": "HDFS-1"
        }, {
            "name": "hive_service",
            "ref": "HIVE-1"
        }, {
            "name": "spark_on_yarn_service",
            "ref": "SPARK_ON_YARN-1"
        }, {
            "name": "cdsw.domain.config",
            "value": "cdsw.{{ hostvars[groups['cdsw_master'][0]]['ansible_host'] }}.nip.io"
        }, {
            "name": "cdsw.master.ip.config",
            "value": "{{ CDSW_IP }}"
        }, {
            "name": "cdsw.enable.mlmodelmetrics.config",
            "value": "true"
        }, {
            "name" : "cdsw.mlmodelmetrics.postgresql.user",
            "value" : "{{ databases.modelops.user }}"
        }, {
            "name" : "cdsw.mlmodelmetrics.postgresql.host",
            "value" : "{{ groups['database'][0] }}"
        }, {
            "name" : "cdsw.mlmodelmetrics.postgresql.dbname",
            "value" : "{{ databases.modelops.name }}"
        }, {
            "name" : "cdsw.mlmodelmetrics.postgresql.pass",
            "value" : "{{ databases.modelops.pass }}"
        }
    ],
    "roleConfigGroups": [
        {
            "refName": "CDSW-1-CDSW_APPLICATION-BASE",
            "roleType": "CDSW_APPLICATION",
            "base": true
        },
        {
          "refName": "CDSW-1-CDSW_MASTER-BASE",
          "roleType": "CDSW_MASTER",
          "base": true
        },
        {
          "refName": "CDSW-1-CDSW_DOCKER-BASE",
          "roleType": "CDSW_DOCKER",
          "base": true,
          "configs": [
            {
              "name": "cdsw.docker.devices.config",
              "value": "{{ DOCKER_DEVICE }}"
            }
          ]
        } 
        
    ]

}